{
  "name": "musclequest",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "APP_VARIANT=development npx expo start",
    "build-dev": "npx eas build --profile development --platform android",
    "build-preview": "npx eas build --profile preview --platform android",
    "build": "npx eas build --profile production --platform android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test-watchAll": "jest --watchAll",
    "lint": "expo lint",
    "eas-build-on-success": "npx bugsnag-eas-build-on-success"
  },
  "jest": {
    "preset": "jest-expo",
    "testTimeout": 15000,
    "setupFiles": [
      "./jestSetupFile.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/android/",
      "/ios/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@?react-native|@react-navigation|expo(nent)?|@expo(nent)?/.*|react-native|@react-native(-community)?/.*)"
    ],
    "moduleNameMapper": {
      "\\.db$": "<rootDir>/__mocks__/fileMocks.js"
    }
  },
  "dependencies": {
